{
  "name": "qedron",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "dotenv -- turbo run build",
    "clean": "turbo run clean",
    "dev": "dotenv -- turbo run dev",
    "db:push":"dotenv -- turbo run db:push",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "init": "pnpm install && git config --global user.email 'biniamdev@gmail.com' && git config --global user.name 'biniamkiros' &&  echo '# import environment variables'> '.env' && code .env"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@9.0.6"
}
